{
  "name": "frontend",
  "version": "0.0.2",
  "author": "Rural Innovation Strategies, Inc.",
  "description": "Amplify Redux React Vite TS template",
  "private": true,
  "type": "module",
  "dependencies": {
    "@aws-amplify/auth": "^6.0.9",
    "@aws-amplify/core": "^6.0.9",
    "@aws-amplify/ui-react": "^6.0.7",
    "@cori-risi/cori.data.api": "github:ruralinnovation/cori.data.api",
    "@reduxjs/toolkit": "^2.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.0",
    "redux": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.2.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.33",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdom": "^21.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.5.1",
    "vitest": "^0.30.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/modules/*.js",
      "!src/plugins/*.js",
      "!src/renv/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/build",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/src/modules",
      "<rootDir>/src/renv",
      "<rootDir>\\build",
      "<rootDir>\\dist",
      "<rootDir>\\node_modules",
      "<rootDir>\\src\\modules",
      "<rootDir>\\src\\renv"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePathIgnorePatterns": [
      "build",
      "dist",
      "node_modules",
      "modules",
      "renv",
      "/build/",
      "/dist/",
      "/node_modules/",
      "/modules/",
      "/renv/",
      "<rootDir>/build",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/src/modules",
      "<rootDir>/src/renv",
      "<rootDir>\\build",
      "<rootDir>\\dist",
      "<rootDir>\\node_modules",
      "<rootDir>\\src\\modules",
      "<rootDir>\\src\\renv"
    ],
    "testPathIgnorePatterns": [
      "build",
      "dist",
      "node_modules",
      "modules",
      "renv",
      "/build/",
      "/dist/",
      "/node_modules/",
      "/modules/",
      "/renv/",
      "<rootDir>/build",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/src/modules",
      "<rootDir>/src/renv",
      "<rootDir>\\build",
      "<rootDir>\\dist",
      "<rootDir>\\node_modules",
      "<rootDir>\\src\\modules",
      "<rootDir>\\src\\renv"
    ],
    "verbose": true
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit"
  }
}
